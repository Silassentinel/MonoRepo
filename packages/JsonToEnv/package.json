{
  "name": "@silassentinel/jsontodotenvironment",
  "private": true,
  "version": "1.0.0",
  "description": "This will convert jsons to dot env files wich makes setup / configs in project possible via jsons and thus via alternative db's in the long run. Developed in Typescript via TDD",
  "main": "Main.ts",
  "scripts": {
    "test": "mocha ./src/test/**/*.spec.ts",
    "start": "ts-node ./src/main.ts",
    "start-dev": "nodemon --exec \"npm run start\"",
    "lint-dev": "nodemon --exec \"npm run eslint:fix\"",
    "eslint": "eslint ./src/*/*.ts",
    "eslint:fix": "eslint --fix ./src/*/*.ts"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "json",
    "dotenv",
    "configuration",
    "multidbsupport"
  ],
  "author": "silassentinel",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.0.2",
    "tslib": "^2.4.0",
    "uuid": "^9.0.0",
    "@silassentinel/toolbox": "file:../toolbox"
  },
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.0.3",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "chai": "^4.3.6",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-security": "^1.5.0",
    "mocha": "^10.0.0",
    "nodemon": "^3.0.0",
    "prettier": "^2.7.1",
    "supertest": "^6.2.4",
    "ts-node": "^10.8.2",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  }
}
